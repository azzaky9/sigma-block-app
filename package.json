{
  "name": "remix-stock-management-systems",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "remix:build": "remix vite:build",
    "remix:dev": "remix vite:dev --mode development",
    "remix:lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "remix:start": "remix-serve ./build/server/index.js",
    "remix:typecheck": "tsc",
    "git:deploy": "pnpm run next:build && git push -u origin main",
    "db:seed": "npx prisma db seed",
    "db:generate": "npx prisma generate client",
    "db:reset": "npx prisma migrate reset",
    "seed:dev": "NODE_ENV=development npx tsx ./app/database/seed.ts",
    "seed:production": "NODE_ENV=production npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --esm app/database/seed"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.13.5",
