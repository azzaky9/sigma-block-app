    "@faker-js/faker": "^8.4.1",
    "@remix-run/dev": "^2.14.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.17.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
